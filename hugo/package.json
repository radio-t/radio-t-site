{
  "description": "Сайт подкаста Радио-Т",
  "scripts": {
    "build": "mix build --production",
    "hugo": "cross-env-shell hugo serve --minify --port ${HUGO_PORT:-1313}",
    "hugo:no-live": "cross-env-shell hugo serve --minify --disableLiveReload --port ${HUGO_PORT:-1313}",
    "dev": "cross-env NODE_ENV=development run-p dev:watch hugo",
    "dev:watch": "mix watch",
    "dev:turbo": "cross-env NODE_ENV=development cross-env MIX_TURBO=1 run-p dev:watch hugo:no-live",
    "serve": "run-s build hugo",
    "size": "npm run build && size-limit",
    "prod": "npm run build",
    "prettier": "prettier '**/*.{js,jsx,ts,tsx,scss}' --write",
    "eslint": "eslint '**/*.{js,jsx,ts,tsx}'"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@sentry/browser": "^8.34.0",
    "axios": "^0.28.1",
    "axios-extensions": "^3.1.3",
    "bootstrap": "^4.6.2",
    "buffer": "^6.0.3",
    "core-js": "^3.1.4",
    "cross-env": "^5.2.0",
    "custom-event-polyfill": "^1.0.7",
    "date-fns": "^2.16.1",
    "imagesloaded": "^4.1.4",
    "intersection-observer": "^0.7.0",
    "laravel-mix": "^6.0.49",
    "light-event-bus": "^1.0.1",
    "lozad": "^1.9.0",
    "mark.js": "^8.11.1",
    "preact": "^10.5.9",
    "purgecss-from-html": "^4.1.1",
    "purgecss-webpack-plugin": "^4.1.3",
    "quicklink": "^2.3.0",
    "resolve-url-loader": "^3.1.0",
    "sass": "^1.80.6",
    "sass-loader": "^8.0.2",
    "scriptjs": "^2.5.9",
    "smooth-scroll": "^16.1.0",
    "smoothscroll-polyfill": "^0.4.4",
    "stimulus": "^1.1.1",
    "ts-loader": "^6.2.2",
    "turbolinks": "^5.2.0",
    "typescript": "^4.9.5",
    "visibilityjs": "^2.0.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^5.96.1",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4"
  },
  "devDependencies": {
    "@size-limit/file": "^4.10.1",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "browser-sync": "^2.29.3",
    "browser-sync-webpack-plugin": "^2.2.2",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-react": "^7.22.0",
    "npm-run-all": "^4.1.5",
    "size-limit": "^4.10.1"
  }
}
